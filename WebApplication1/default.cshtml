@using WebApplication1.App_Start
@inherits ViewPage
@{
    Layout = "/Layouts/_Root";
}


@{
    Layout = "/Layouts/_Root";
    var currentSession = SessionAs<UserAuthSession>();

    string error = null;
    if (Request.QueryString["error"] != null)
    {
        error = Request.QueryString["error"];
    }
}

Current Session UserAuthName : @currentSession.UserAuthName <br />
Current Session Id: @currentSession.Id <br />
Current Session UserId (custom field, is null when using redis): @currentSession.UserId <br />
